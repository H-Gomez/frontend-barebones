{
  "name": "frontend-barebones-",
  "version": "1.0.0",
  "description": "A barebones front-end development environment which handles the setup of ESLint, Prettier, Parcel and other development dependencies.",
  "main": "index.js",
  "scripts": {
    "dev": "parcel src/index.html",
    "format": "prettier --write \"src/**/*.{js,css,html,json}\"",
    "lint": "eslint src/**.js",
    "test": "jest",
    "test:coverage": "jest --verbose --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/H-Gomez/frontend-barebones-.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/H-Gomez/frontend-barebones-/issues"
  },
  "homepage": "https://github.com/H-Gomez/frontend-barebones-#readme",
  "devDependencies": {
    "eslint": "^5.13.0",
    "eslint-config-prettier": "^4.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.14.3",
    "jest": "^24.8.0",
    "parcel-bundler": "^1.12.3",
    "prettier": "^1.16.4"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
